<openerp>
	<data>
	
		<record id="form_crm_categ" model="ir.ui.view">
			<field name="name">crm.form_crm_categ</field>
			<field name="model">crm.case.categ</field>
			<field name="inherit_id" ref="crm.crm_case_categ-view" />
			<field name="arch" type="xml">
				<field name="name" position="after">
					<field name="code" select="1" />
				</field>
			</field>
		</record>

		<record id="tree_crm_categ" model="ir.ui.view">
			<field name="name">crm.tree_crm_categ</field>
			<field name="model">crm.case.categ</field>
			<field name="inherit_id" ref="crm.crm_case_categ_tree-view" />
			<field name="arch" type="xml">
				<field name="name" position="after">
					<field name="code" />
				</field>
			</field>
		</record>
		
	</data>
</openerp>