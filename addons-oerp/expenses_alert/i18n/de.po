#. module: expenses_alert
#: model:email.template,body_html:expenses_alert.email_template_expense_alert
msgid ""
"\n"
"                <div style=\"font-family: 'Lucica Grande', Ubuntu, Arial, "
"Verdana, sans-serif; font-size: 11pt;\">\n"
"                   <p>${object.partner_id.mail_salutation}, </p>\n"
"                   <p><strong>following effort will be invoiced:</strong></"
"p>\n"
"                   <table border=\"1\">\n"
"                        <tr>\n"
"                            <th>Date</th>\n"
"                            <th>Description</th>\n"
"                            <th>Amount</th>\n"
"                            <th>Unit</th>\n"
"                            <th>Invoice</th>\n"
"                        </tr>\n"
"                        % for line in object.expense_alert_line_ids:\n"
"                        <tr>\n"
"                            <td>${formatLang(line.date,date=True)}</td>\n"
"                            <td>${line.name}</td>\n"
"                            <td>${formatLang(line.unit_amount)}</td>\n"
"                            <td>${line.product_uom_id and line."
"product_uom_id.name or ''}</td>\n"
"                            <td>${line.to_invoice.name}</td>\n"
"                        </tr>\n"
"                        % endfor\n"
"                    </table>                            \n"
"                    <p>Best Regards</p>\n"
"                    <p>${object.company_id.name}</p>\n"
"                </div>\n"
"        "
msgstr ""
"\n"
